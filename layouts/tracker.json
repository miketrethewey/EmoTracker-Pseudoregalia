{
  "item_grid": {
    "type": "array",
    "content": [
      {"type": "layout", "key": "powerups_grid"},
      {"type": "layout", "key": "aspects_grid"},
      {"type": "layout", "key": "consumables_grid"}
    ]
  },
  "settings_grid": {
    "type": "array",
    "margin": "5",
    "content": [
      {
        "type": "group",
        "header": "Settings",
        "content": [
          {
            "type": "array",
            "content": [
              {
                "type": "group",
                "header": "Options",
                "margin": "0, 0, 0, 10",
                "content": [{"type": "layout", "key": "set_options"}]
              },
              {
                "type": "group",
                "header": "Tricks",
                "margin": "0, 0, 0, 10",
                "content": [{"type": "layout", "key": "set_tricks"}]
              }
            ]
          }
        ]
      }
    ]
  },
  "tracker_default": {
    "type": "container",
    "background": "#085810",
    "content": {
      "type": "dock",
      "dropshadow": false,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#50212121",
                "layout": "settings_grid"
              },
              "content": [
                {
                  "type": "layout",
                  "margin": "0, 0, 0, 0",
                  "key": "item_grid"
                }
              ]
            },
            {
              "type": "group",
              "header": "Pinnned Locations",
              "dock": "left",
              "content": {
                "type": "scroll",
                "horizontal_scrollbar_visibility": "disabled",
                "vertical_scrollbar_visibility": "auto",
                "content": {
                  "type": "recentpins",
                  "style": "wrap",
                  "h_alignment": "stretch",
                  "v_alignment": "stretch",
                  "orientation": "vertical",
                  "compact": true
                }
              }
            }
          ]
        },
        {
          "type": "dock",
          "v_alignment": "stretch",
          "margin": "0,0,0,0",
          "content": [{"type": "layout", "key": "tabs_maps"}]
        }
      ]
    }
  },
  "tracker_vertical": {
    "type": "container",
    "background": "#28622e",
    "content": {
      "type": "dock",
      "dropshadow": false,
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#50212121",
                "layout": "settings_grid"
              },
              "content": {
                "type": "layout",
                "key": "item_grid"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "dock": "right",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "vertical",
                "compact": true
              }
            }
          ]
        },
        {
          "type": "dock",
          "v_alignment": "stretch",
          "margin": "0,0,0,0",
          "content": [{"type": "layout", "key": "tabs_maps"}]
        }
      ]
    }
  }
}
